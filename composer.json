{
    "name": "giulioprinaricotti/dandelion-php",
    "description": "Unofficial Swagger Generated PHP Client for Dandelion API Edit",
    "keywords": [
        "dandelion",
        "nlp",
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/giulioprinaricotti/dandelion-php",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Giulio Prina Ricotti",
            "homepage": "https://github.com/giulioprinaricotti"
        },
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/" }
    }
}
